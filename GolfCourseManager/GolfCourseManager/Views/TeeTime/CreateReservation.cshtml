@model GolfCourseManager.ViewModels.ReservationCreatedViewModel
@{
	ViewBag.Title = "Reservations Created";
}

@section Scripts {
	<script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
	<script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
}


<div class="row">
	<div class="col-md-4 col-md-offset-4">
		<h1>Golf Course Manager - @ViewBag.Title</h1>
		<ul class="bg-success">
			@if (Model.Reservations == null)
			{
				<li class="alert-info">No reservations created.</li>
			}
			else
			{
				foreach (var reservationTime in Model.Reservations)
				{
					<li class="text-success">@reservationTime.ToString()</li>
				}
			}
		</ul>
	</div>
</div>
